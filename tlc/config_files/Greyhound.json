{"webpage":{
	"name": "Greyhound",
	"uri_start": "http://www.greyhound.com/",
	"uri_end": "",
	"travel_type": 3,
	"sleep_time": 3,
	"date_span_start": 5,
	"date_span_finish": 5,
	"decimal_mark": ".",
	"thousands_mark":",",
	"frequency_format":[],
	"date_format": "%m/%d/%Y",
	"page_type":2,
	"header_parameters":{},
	"UTC": -3,
	"currency": "USD",
	"inputs": {
		"wait":5,
		"loading_time":15,
		"buttons":[
			{"field_type":"input", "id":"fromLocation", "data":["origin_local_code"]},
      {"field_type":"input", "id":"toLocation", "data":["destination_local_code"]},
      {"field_type":"input", "id":"datepicker-from", "data": ["departure"]},
      {"field_type":"input", "id":"fare-search-btn", "data":["click"]}
      ]},
	"iterators":{},
	"extraction_tags":{
			"departure":{
				"fields":[
					{"tag_type": "li", "field_type": "class", "name": "fare"},
					{"tag_type": "p", "field_type": "class", "name": "trip-time trip-time-lg","position": 0}
				],
				"format":"(\\d?\\d):(\\d\\d)\\s(\\w\\w)",
				"formula":"if ($3 == 'am') : x=[$1, $2]\nelse: x=[(int($1) +12) % 24, $2]"
			},
			"arrival":{
				"fields":[],
				"format":"",
				"formula":""
			},
			"price":{
				"fields":[
					{"tag_type": "li", "field_type": "class", "name": "fare"},
					{"tag_type": "div", "field_type": "class", "name": "row fare-prices"},
					{"tag_type": "span", "field_type": "class", "name": "price","position": 0}],
				"format":"(\\d+)",
				"formula":""
			},
			"duration": {
				"fields":[
					{"tag_type": "li", "field_type": "class", "name": "fare"},
					{"tag_type": "p", "field_type": "class", "name": "trip-duration","position": 0}],
			"format":"(\\d?\\d)h\\s?(\\d?\\d)m",
				"formula":""
			},
			"travel_agency": {
				"fields":[],
				"format":"Greyhound",
				"formula":""
			},
			"frequency": {
				"fields":[],
				"format":"",
				"formula":""
			}
		}
	}
}
